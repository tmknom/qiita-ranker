# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# ユーザ一覧
GET        /qiita/users                       presentation.controller.QiitaUserController.list()

# ユーザのイイね一覧
GET        /qiita/users/contributions         presentation.controller.QiitaUserContributionController.list()
GET        /qiita/users/contributions2        presentation.controller.QiitaUserContributionController.list2()

# ヘルスチェック
GET        /health_check                      library.controller.HealthCheckController.index

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file                      controllers.Assets.versioned(path="/public", file: Asset)
